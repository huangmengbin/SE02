#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_serve@11.3.2@serve/bin/serve.js" "$@"
  ret=$?
else 
  node  "$basedir/../_serve@11.3.2@serve/bin/serve.js" "$@"
  ret=$?
fi
exit $ret
